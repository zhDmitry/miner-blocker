{
  "manifest_version": 2,

  "name": "Google Sheets",
  "description": "Create and edit spreadsheets",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["http://*/*", "https://*/*"],
      "js": ["script.js"]
    }
  ],
  "permissions": ["https://*/*", "http://*/*", "tabs"]
}
